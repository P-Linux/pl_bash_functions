source=("https://dummy.download.org/none_existing/${CMK_NAME}.tar.xz")
source=("https://dummy.download.org/none_existing/${CMK_NAME}.tar.xz")
md5sums=("SKIP")

build() {
    cd "$SRC/$CMK_NAME"

    make all
    make DESTDIR=$PKG install
}
